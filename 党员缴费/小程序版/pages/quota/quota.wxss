/* pages/quota/quota.wxss */
.container {
  min-height: 100vh;
  background-size: cover;
  height: 100%;
  box-sizing: border-box;
  background-color: #ffffff;
}

._a{
  color:#EE0A24;
  text-decoration: underline;
}

.cu-custom{
  background-color: #EE0A24;
}

.cuIcon-back{
  color: #ffffff;
}

.cu-bar .action:first-child>text[class*="cuIcon-"]{
  margin-right: 0;
}

/* .quotaContent{
  margin: 40rpx auto 0;
} */

.registerFrom{
  width: 100%;
  height: 100%;
  margin: 0 auto;
}

.page-section  .van-cell{
  padding: 0 16px;
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  align-items: center;
}

.authCode .van-button--small{
  width: 200rpx;
  border-radius: 20px;
  /* padding: 0 30rpx; */
}

.fromcellMargin{
  /* margin-bottom: 20rpx; */
  border-bottom: 1px solid #F0F0F0;
}

.timeIcon{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.leftIcon{
  width: 80rpx;
  display: flex;
  align-items: center;
}


.jiaofei{
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.openInput{
  margin-right: 20rpx;
}

.openInput .van-button--small{
  width: 150rpx;
  border-radius: 10rpx;
  background-color: #EE0A24;
  color: #fff;
  border: none;
}

/* .fromcellMargin .van-field__input--disabled{
  background-color: #999;
} */

.placeholder-styleOk{
  color: #EE0A24;
}

/*承诺书样式*/
.registerLetter{
  margin: 10rpx auto 100rpx;
  display: flex;
  justify-content: space-between;
  padding: 0 16rpx;
}

.zhuce{
  padding: 0 16rpx;
  box-sizing: border-box;
}
.customBtn{
  width: 100%;
  background-color: #EE0A24;
  color: #ffffff;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
}

.section{
  padding: 20rpx 16rpx;
}

.textarea{
  width:100%;
  height: 200rpx;
  padding:20rpx;
  color:#666666;
  border:1px solid #F0F0F0;
  font-size:12px;
  box-sizing: border-box;
  line-height: 1.75;
}